! rxvt-unicode
URxvt*termName:			rxvt
URxvt*reverseVideo:		false
URxvt*scrollBar:		false
URxvt*font:				xft:Terminus:size=9:antialias=true
URxvt*boldfont:			xft:Terminus Bold:size=9:antialias=true
URxvt*italicfont:		xft:Terminus Oblique:size=9:antialias=true
URxvt*geometry:			120x42
URxvt*depth:			32
URxvt*fading:			40
URxvt*foreground:		#FCAF3E
URxvt*background:		[90]#000000
URxvt*borderless:		1
URxvt*loginShell:		true
URxvt*iconFile:			/usr/share/pixmaps/terminal_icon.png

*cursorColor:           #FCAF3E
!black
*color0:                #1c1c1c
*color8:                #6d6d6d
!red
*color1:                #D32020
*color9:                #DA5050
!green
*color2:                #66aa11
*color10:               #80ff00
!yellow
*color3:                #cdd24a
*color11:               #e0e565
!blue
*color4:                #2C31FF
*color12:               #454AE8
!magenta
*color5:                #7e40a5
*color13:               #8a5fa5
!cyan
*color6:                #125889
*color14:               #0380CB
!white
*color7:                #bbbbbb
*color15:               #dddddd
