alignment bottom_left
background no
border_width 1
cpu_avg_samples 2
default_color white
default_shade_color #858687
default_outline_color black
double_buffer yes
draw_borders no
draw_graph_borders yes
draw_outline no
draw_shades yes
use_xft yes
xftfont Droid Sans Mono:size=8
xftalpha 1
gap_x 5 
gap_y 635
minimum_size 5 5
net_avg_samples 2
no_buffers yes
out_to_console no
out_to_stderr no
extra_newline no
own_window yes
own_window_transparent yes
own_window_class Conky
own_window_type desktop
own_window_hints sticky,skip_taskbar
stippled_borders 8
update_interval 1
uppercase no
use_spacer none
show_graph_scale no
show_graph_range no
cpu_avg_samples 2
net_avg_samples 2
if_up_strictness address
temperature_unit fahrenheit

TEXT
${color #A2A2A2}$nodename ${alignr}linux-$kernel
${time %a, %d %b %Y %T} ${alignr}${weather http://weather.noaa.gov/pub/data/observations/metar/stations/ kord temperature}Â° ${weather http://weather.noaa.gov/pub/data/observations/metar/stations/ kord humidity}%
${color #858687}${stippled_hr 1,1}
